{
  "name": "cypress-slack-video-upload-action",
  "version": "1.2.0",
  "private": true,
  "description": "Github action that takes the generated screenshots and videos from a Cypress test and uploads them to Slack",
  "main": "lib/main.js",
  "engines": {
    "node": ">=20.19.5"
  },
  "scripts": {
    "build": "tsc",
    "dev": "concurrently --raw \"tsc --watch\" \"ncc build --source-map --license licenses.txt --watch\"",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@slack/web-api": "^7.12.0",
    "walk-sync": "^4.0.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.5",
    "@typescript-eslint/parser": "^8.47.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.1",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.2.0",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "js-yaml": "^4.1.1",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}